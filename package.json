{
  "name": "@web.worker/create-astro-exe",
  "type": "module",
  "version": "1.2.5",
  "description": "Create Astro applications with a single command",
  "author": "Web Worker Team",
  "license": "MIT",
  "homepage": "https://github.com/web-worker/create-astro-exe#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/web-worker/create-astro-exe.git"
  },
  "bugs": {
    "url": "https://github.com/web-worker/create-astro-exe/issues"
  },
  "keywords": [
    "astro",
    "template",
    "scaffold",
    "cli",
    "create-app"
  ],
  "bin": {
    "create-astro-exe": "dist/create-astro-exe.cjs"
  },
  "files": [
    "README.md",
    "bin",
    "dist",
    "lib",
    "template/.gitignore.template",
    "template/.npmrc.template",
    "template/AGENTS.md",
    "template/README.md",
    "template/astro.config.mjs",
    "template/config",
    "template/data",
    "template/db",
    "template/package.json",
    "template/public",
    "template/scripts",
    "template/src",
    "template/tsconfig.json"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "scripts": {
    "build": "tsdown",
    "test": "vitest run",
    "types": "tsc --noEmit",
    "lint": "eslint .",
    "format": "eslint --fix",
    "prepublishOnly": "npm run build && npm run lint && npm run types && npm run test"
  },
  "dependencies": {
    "@types/node": "^22.19.0",
    "chalk": "^5.3.0",
    "fs-extra": "^11.2.0",
    "inquirer": "^9.2.12",
    "ora": "^7.0.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.1.0",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "eslint": "^9",
    "eslint-plugin-format": "^1.0.2",
    "prettier": "^3.1.1",
    "tsdown": "^0.16.1",
    "typescript": "^5.9.3",
    "vitest": "^1.1.0"
  }
}
